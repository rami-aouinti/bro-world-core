{
	"name": "bro-world/core-bundle",
	"description": "Core utilities & services for Bro World projects",
	"type": "symfony-bundle",
	"homepage": "https://github.com/rami-aouinti/bro-world-core",
	"license": "proprietary",
	"authors": [
		{
			"name": "Rami Aouinti",
			"email": "rami.aouinti@gmail.com",
			"homepage": "https://github.com/rami-aouinti",
			"role": "Developer"
		}
