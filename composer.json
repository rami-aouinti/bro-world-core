                "mark-gerarts/automapper-plus-bundle": "^2.4"
        },
	"suggest": {
		"doctrine/orm": "^2.14 || ^3.0"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.1.x-dev"
		}
	},
	"support": {
		"issues": "https://github.com/rami-aouinti/bro-world-core/issues"
